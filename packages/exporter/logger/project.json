{
  "name": "exporter-logger",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/exporter/logger/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/exporter/logger",
        "main": "packages/exporter/logger/src/index.ts",
        "tsConfig": "packages/exporter/logger/tsconfig.lib.json",
        "assets": ["packages/exporter/logger/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/exporter/logger/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/exporter/logger/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
